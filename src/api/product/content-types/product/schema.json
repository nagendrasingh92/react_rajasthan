{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "float"
    },
    "currency": {
      "type": "string"
    },
    "product_images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "outlet": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::outlet.outlet",
      "inversedBy": "products"
    },
    "product_status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published",
        "out_of_stock"
      ]
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "product"
    },
    "totalProduct": {
      "type": "integer"
    },
    "stockQuantity": {
      "type": "integer"
    }
  }
}
